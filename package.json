{
    "devDependencies": {
      "@typescript-eslint/eslint-plugin": "^5.40.0",
      "@typescript-eslint/parser": "^5.40.0",
      "eslint": "^8.25.0",
      "eslint-config-metarhia": "^8.1.0",
      "eslint-plugin-sonarjs": "^0.16.0",
      "prettier": "^1.19.1",
      "husky": "^8.0.1",
